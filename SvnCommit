# Do not include */*.vcproj in this automatically edited list
list="\
	CppAD/local/ADTape.h \
	CppAD/local/Declare.h \
	CppAD/local/AD.h \
	CppAD/local/CppADError.h \
	CppAD/local/Fun.h \
	CppAD/local/admap.h \
	omh/ExampleList.omh \
	Example/admap.cpp \
	Example/Example.cpp \
	Example/Makefile.am \
	CppAD/local/admap.h \
"
for file in $list
do
	sed -f SvnCommit.sed < $file > junk
	echo $file
	diff $file junk
	mv $file ~/trash 
	mv junk $file
done
svn add Example/admap_map.cpp
sed -e "/^[^#]/d" -e "s/^[#] *//" < SvnCommit > svn.log
svn commit --file svn.log \
	SvnCommit \
	Example/Example.vcproj \
	$list
