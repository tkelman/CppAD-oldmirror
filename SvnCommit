# Branch admap: documentation files for admap routines
# (these routines are not yet implemented or tested).
# 
# SvnCommit: this file.
# admap.omh: documentation node for admap class.
# admap_member.omh: documentation for admap member funcitons.
# -----------------------------------------------
list="\
	omh/admap.omh \
	omh/admap_member.omh \
	Doc.omh
"
for file in $list
do
	sed -f SvnCommit.sed < $file > junk
	echo $file
	diff $file junk
	mv $file ~/trash 
	mv junk $file
done
sed -e "/^[^#]/d" -e "s/^[#] *//" < SvnCommit > svn.log
svn add omh/admap.omh
svn add omh/admap_member.omh
svn commit --file svn.log \
	SvnCommit \
	$list
