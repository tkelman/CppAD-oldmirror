# Branch admap: improve documentaiton and plan for admap template class
# 
# SvnCommit: this file.
# admap.omh: use normal font for the empty mapping state.
# admap.h: document input / output mapping state for each member function.
# -----------------------------------------------
list="\
	CppAD/local/admap.h
	omh/admap.omh \
"
for file in $list
do
	sed -f SvnCommit.sed < $file > junk
	echo $file
	diff $file junk
	mv $file ~/trash 
	mv junk $file
done
sed -e "/^[^#]/d" -e "s/^[#] *//" < SvnCommit > svn.log
svn commit --file svn.log \
	SvnCommit \
	$list
